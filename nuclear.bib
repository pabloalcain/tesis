	pages = {26--41},
	file = {ACS Full Text PDF w/ Links:/home/palcain/.mozilla/firefox/7y8dc34m.default/zotero/storage/KKVFFE2E/Chodera et al. - 2007 - Use of the Weighted Histogram Analysis Method for .pdf:application/pdf;ACS Full Text Snapshot:/home/palcain/.mozilla/firefox/7y8dc34m.default/zotero/storage/32STGB5S/ct0502864.html:text/html}
}

