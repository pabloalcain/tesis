	pages = {2382--2389},
	file = {Salacuse et al. - 1996 - Finite-size effects in molecular dynamics simulati.html:/home/palcain/.mozilla/firefox/7y8dc34m.default/zotero/storage/UTDIASBN/Salacuse et al. - 1996 - Finite-size effects in molecular dynamics simulati.html:text/html;Salacuse et al. - 1996 - Finite-size effects in molecular dynamics simulati.pdf:/home/palcain/.mozilla/firefox/7y8dc34m.default/zotero/storage/7HHSBBHU/Salacuse et al. - 1996 - Finite-size effects in molecular dynamics simulati.pdf:application/pdf}
}

