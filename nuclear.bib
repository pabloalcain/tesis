	author = {Ma, Y. G. and Natowitz, J. B. and Wada, R. and Hagel, K. and Wang, J. and Keutgen, T. and Majka, Z. and Murray, M. and Qin, L. and Smith, P. and Alfaro, R. and Cibor, J. and Cinausero, M. and Masri, Y. El and Fabris, D. and Fioretto, E. and Keksis, A. and Lunardon, M. and Makeev, A. and Marie, N. and Martin, E. and Martinez-Davalos, A. and Menchaca-Rocha, A. and Nebbia, G. and Prete, G. and Rizzi, V. and Ruangma, A. and Shetty, D. V. and Souliotis, G. and Staszel, P. and Veselsky, M. and Viesti, G. and Winchester, E. M. and Yennello, S. J.},
